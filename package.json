{
  "name": "@storybook-community/storybook-source-link",
  "version": "5.0.0",
  "description": "Provides a link to the story's source in the toolbar",
  "keywords": [
    "storybook-addons",
    "storybook-community",
    "storybook",
    "code",
    "source",
    "link"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/storybook-community/storybook-source-link"
  },
  "type": "module",
  "author": "Jonathan Sirrine <sirrineprogramming@gmail.com>",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsup",
    "build:watch": "yarn run build -- --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "start": "run-p build:watch \"storybook --quiet\"",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "yarn run build && auto shipit",
    "eject-ts": "zx scripts/eject-typescript.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@storybook/addon-docs": "next",
    "@storybook/addon-essentials": "next",
    "@storybook/addon-interactions": "next",
    "@storybook/addon-links": "next",
    "@storybook/blocks": "next",
    "@storybook/icons": "^1.4.0",
    "@storybook/react": "next",
    "@storybook/react-vite": "next",
    "@storybook/test": "next",
    "@storybook/theming": "next",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.18",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@types/semver": "^7.7.0",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.4.1",
    "auto": "^11.3.0",
    "boxen": "^8.0.1",
    "dedent": "^1.6.0",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.1",
    "prettier": "^3.5.3",
    "prompts": "^2.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "storybook": "next",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.3",
    "zx": "^8.5.4"
  },
  "peerDependencies": {
    "@storybook/icons": "^1.4.0",
    "storybook": "^9.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.ts"
    ],
    "previewEntries": [
      "src/preview.ts"
    ]
  },
  "storybook": {
    "displayName": "Source Link",
    "supportedFrameworks": [
      "supported-frameworks"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  },
  "packageManager": "yarn@4.0.1"
}
